{
  "configuration": {
    "name": "GameServer",
    "status": "info",
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%d{ISO8601} %highlight{%-5level} %-25logger{1}%notEmpty{ [%marker]} - %msg%throwable{20}%n"
        }
      }
    },
    "loggers": {
      "root": {
        "level": "trace",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}